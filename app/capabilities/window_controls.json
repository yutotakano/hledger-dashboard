{
  "identifier": "window-controls",
  "description": "permissions that are needed for window controls to work",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "os:allow-os-type",
    "window:allow-toggle-maximize",
    "window:allow-set-fullscreen",
    "window:allow-minimize",
    "window:allow-close"
  ]
}
